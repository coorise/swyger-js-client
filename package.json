{
  "name": "@swyger/client",
  "version": "0.1.0",
  "description": "Swyger Client is used for Swyger Server as consumer Rest API. Visit the master branch: https://github.com/coorise/swyger-js-client.git",
  "type": "module",
  "main": "src/index.js",
  "publishConfig": {
    "ignore": [
      ".git/",
      ".idea/",
      ".backup/",
      "v*.zip/",
      "static/",
      "node_modules"
    ],
    "registry": "https://npm.pkg.github.com/swyger"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/coorise/swyger-js-client.git"
  },
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "dev": "cross-env NODE_ENV=dev node  ./replace-src.js && vite",
    "build": "cross-env NODE_ENV=prod BUILD_MODE=bundle-minify  node ./replace-src.js && vite build -- --minify --bundle",
    "build:dev": "cross-env NODE_ENV=dev node  ./replace-src.js && vite build",
    "build:bundle": "cross-env NODE_ENV=dev BUILD_MODE=bundle  node ./replace-src.js && vite build -- --bundle",
    "start": "vite preview"
  },
  "keywords": ["Client","RestAPI","API","Swyger","Swyger Client","Boilerplate","CMS"],
  "author": "Agglomy",
  "license": "MIT",
  "dependencies": {
    "acebase": "^1.29.0",
    "axios": "^1.4.0",
    "cross-env": "^7.0.3",
    "dottie": "^2.0.3",
    "eventemitter3": "^5.0.1",
    "jwt-decode": "^3.1.2",
    "socket.io-client": "^4.6.1",
    "uuid": "^9.0.0"
  },
  "devDependencies": {
    "@rollup/plugin-node-resolve": "^15.0.1",
    "glob-all": "^3.3.1",
    "rollup-plugin-commonjs": "^10.1.0",
    "sass": "^1.57.1",
    "vite": "^3.2.3",
    "vite-plugin-compression": "^0.5.1",
    "vite-plugin-singlefile": "^0.13.5"
  }
}
